THEN
	RETURN((data_type_non_quantitative_int_type(
		vcv_range.parameter_type)[1].domain
		.its_values[1].value_code <= vcv_range.range_lobound)
		AND (vcv_range.range_hibound <=
		data_type_non_quantitative_int_type(
