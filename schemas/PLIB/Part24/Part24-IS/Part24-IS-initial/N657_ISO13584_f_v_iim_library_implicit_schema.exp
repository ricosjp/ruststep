WHERE
	WR1: EXISTS(http_directory) XOR
		('ISO13584_F_V_IIM_LIBRARY_IMPLICIT_SCHEMA.SUPPLIER_BSU_RELATED_CONTENT'

		IN TYPEOF(SELF.unit_of.content_of.content_of));
