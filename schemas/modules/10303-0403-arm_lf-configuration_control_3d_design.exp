ENTITY Geometric_tolerance
  ABSTRACT SUPERTYPE OF (ONEOF (
                       ANGULARITY_TOLERANCE,
                       CIRCULAR_RUNOUT_TOLERANCE,
                       COAXIALITY_TOLERANCE,
                       CONCENTRICITY_TOLERANCE,
                       CYLINDRICITY_TOLERANCE,
                       FLATNESS_TOLERANCE,
                       LINE_PROFILE_TOLERANCE,
                       PARALLELISM_TOLERANCE,
                       PERPENDICULARITY_TOLERANCE,
                       POSITION_TOLERANCE,
                       ROUNDNESS_TOLERANCE,
                       STRAIGHTNESS_TOLERANCE,
                       SURFACE_PROFILE_TOLERANCE,
                       SYMMETRY_TOLERANCE,
                       TOTAL_RUNOUT_TOLERANCE));
  applied_to : Shape_element;
  modification : Tolerance_condition;
  name : OPTIONAL STRING;
  qualifying_note : OPTIONAL STRING;
  segment_size : OPTIONAL Value_with_unit;
  significant_digits : OPTIONAL INTEGER;
  tolerance_value : Value_with_unit;
  value_determination : OPTIONAL STRING;
WHERE
  WR1: (NOT (EXISTS(segment_size))) OR ('CONFIGURATION_CONTROL_3D_DESIGN_ED2_ARM.LENGTH_MEASURE' IN TYPEOF(segment_size.value_component));
  WR2: (NOT (EXISTS(tolerance_value))) OR ('CONFIGURATION_CONTROL_3D_DESIGN_ED2_ARM.LENGTH_MEASURE' IN TYPEOF(tolerance_value.value_component));

  WR3: (NOT (EXISTS(significant_digits))) OR (significant_digits > 0);
END_ENTITY;



