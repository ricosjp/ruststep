WHERE
  WR1: SIZEOF(QUERY(pvr <* QUERY ( r <* USEDIN(SELF, 'CONFIGURATION_CONTROL_3D_DESIGN_ED2_ARM.REPRESENTATION.ITEMS')| 'CONFIGURATION_CONTROL_3D_DESIGN_ED2_ARM.PROPERTY_VALUE_REPRESENTATION' IN TYPEOF(r)) | EXISTS(pvr.context_of_items.units) )) > 0;

END_ENTITY;



ENTITY Numerical_item_with_unit
